<odoo>

    <record model="ir.ui.view" id="mis_committed_purchase_tree_view">
        <field name="name">mis.committed.purchase.tree (in mis_builder_demo)</field>
        <field name="model">mis.committed.purchase</field>
        <field name="arch" type="xml">
            <tree>
                <field name="line_type"/>
                <field name="date"/>
                <field name="name"/>
                <field name="account_id"/>
                <field name="analytic_account_id" groups="analytic.group_analytic_accounting"/>
                <field name="analytic_tag_ids" widget="many2many_tags" groups="analytic.group_analytic_accounting"/>
                <field name="debit"/>
                <field name="credit"/>
            </tree>
        </field>
    </record>

</odoo>
